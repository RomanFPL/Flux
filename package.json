{
    "name": "SFM-dashboard",
    "version": "4.0.0",
    "private": true,
    "scripts": {
        "dev": "next dev",
        "build": "next build",
        "start": "next start",
        "lint": "eslint --fix ./src/ --ext .js,.jsx,.ts,.tsx",
        "format": "prettier --write \"./src/**/*.{js,jsx,ts,tsx,css,md}\"",
        "generate:api": "openapi-generator-cli generate -i https://fleetmanagerbackapi.azurewebsites.net/swagger/v1/swagger.json -g typescript-axios -o ./src/services/openApi --skip-validate-spec",
        "test": "jest --watch",
        "test:ci": "jest --ci",
        "preci": "npm run lint && npm run test:ci",
        "clean": "rimraf .next",
        "clean:all": "rimraf .next node_modules"
    },
    "dependencies": {
        "@emotion/cache": "^11.11.0",
        "@emotion/react": "^11.11.1",
        "@emotion/styled": "^11.11.0",
        "@fontsource/roboto": "^5.0.8",
        "@mui/material": "^5.15.20",
        "@mui/material-nextjs": "^5.15.11",
        "@mui/x-date-pickers": "^7.20.0",
        "@reduxjs/toolkit": "^2.2.5",
        "axios": "^1.7.2",
        "chart.js": "^4.4.3",
        "chartjs-plugin-annotation": "^3.0.1",
        "chartjs-plugin-datalabels": "^2.2.0",
        "chartjs-plugin-zoom": "^2.0.1",
        "date-fns": "^4.1.0",
        "next": "13.4.12",
        "next-intl": "^3.14.1",
        "react": "18.2.0",
        "react-chartjs-2": "^5.2.0",
        "react-date-range": "^2.0.1",
        "react-dom": "18.2.0",
        "react-hook-form": "^7.52.2",
        "react-redux": "^9.1.2",
        "redux": "^5.0.1"
    },
    "devDependencies": {
        "@openapitools/openapi-generator-cli": "^2.13.4",
        "@testing-library/jest-dom": "^6.0.0",
        "@testing-library/react": "^14.0.0",
        "@testing-library/user-event": "^14.4.3",
        "@types/lodash": "^4.17.7",
        "@types/node": "20.4.6",
        "@types/react": "18.3.3",
        "@types/react-date-range": "^1.4.9",
        "@types/react-dom": "18.2.7",
        "@types/testing-library__jest-dom": "^5.14.9",
        "eslint": "8.46.0",
        "eslint-config-next": "^13.4.12",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-prettier": "^5.1.3",
        "eslint-plugin-testing-library": "^6.2.2",
        "jest": "^29.6.2",
        "jest-environment-jsdom": "^29.6.2",
        "prettier": "^3.3.2",
        "rimraf": "^5.0.1",
        "typescript": "5.1.6"
    }
}
